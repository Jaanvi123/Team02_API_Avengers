						"exec": [
							"// Fail-fast guard: block execution when userLoginEmailId is \"team2@gmail.com\"",
							"(function guardTeam2Reset() {",
							"    try {",
							"        // 1) Read raw body text from the request",
							"        var raw = pm.request.body && pm.request.body.mode === 'raw' ? pm.request.body.raw : '';",
							"        if (!raw) { return; }",
							"",
							"        // 2) Resolve optional variable placeholders like {{userEmail}}",
							"        //    Use the same interpolation Postman uses for variables",
							"        var resolved = pm.variables.replaceIn(raw);",
							"",
							"        // 3) Parse JSON safely",
							"        var payload;",
							"        try {",
							"            payload = JSON.parse(resolved);",
							"        } catch (e) {",
							"            // If not valid JSON, do not block; just allow request to proceed",
							"            return;",
							"        }",
							"",
							"        // 4) Check condition and throw to halt execution",
							"        if (payload && payload.userLoginEmailId === 'team2@gmail.com') {",
							"            throw new Error('Access denied to reset Team2 password');",
							"        }",
							"        // 5) Otherwise, do nothing and allow the request to proceed",
							"    } catch (err) {",
							"        // Re-throw to ensure the run fails fast for this guard",
							"        throw err;",
							"    }",
							"})();",
							"",
							"// Existing script retains current functionality below",
							"function generatePassword(minLen = 8, maxLen = 32) {",
							"    const uppercase = \"ABCDEFGHIJKLMNOPQRSTUVWXYZ\";",
							"    const lowercase = \"abcdefghijklmnopqrstuvwxyz\";",
							"    const numbers   = \"0123456789\";",
							"    const special   = \"$@$!%*?&\";   // special chars allowed by your regex",
							"",
							"    const allChars = uppercase + lowercase + numbers + special;",
							"",
							"    // Random length between min and max",
							"    const length = Math.floor(Math.random() * (maxLen - minLen + 1)) + minLen;",
							"",
							"    let password = \"\";",
							"",
							"    // Ensure the password includes each required character type",
							"    password += uppercase[Math.floor(Math.random() * uppercase.length)];",
							"    password += lowercase[Math.floor(Math.random() * lowercase.length)];",
							"    password += numbers[Math.floor(Math.random() * numbers.length)];",
							"    password += special[Math.floor(Math.random() * special.length)];",
							"",
							"    // Fill the remaining characters randomly",
							"    for (let i = password.length; i < length; i++) {",
							"        password += allChars[Math.floor(Math.random() * allChars.length)];",
							"    }",
							"",
							"    // Shuffle characters so the required chars aren't predictable",
							"    password = password",
							"        .split(\"\")",
							"        .sort(() => Math.random() - 0.5)",
							"        .join(\"\");",
							"",
							"    return password;",
							"}",
							"",
							"// Generate a valid password",
							"let newPassword = generatePassword(8, 32);",
							"",
							"// Store it in environment",
							"pm.environment.set(\"password\", newPassword);",
							"",
							"console.log(\"Generated Password:\", newPassword);",
							""
