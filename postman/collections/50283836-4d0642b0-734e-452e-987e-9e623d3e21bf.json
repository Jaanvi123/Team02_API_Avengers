		{
			"name": "Team02-User Sign In",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "5cabcb04-4a31-4736-ac3e-df9f6d4aa457",
						"exec": [
							"// Safely parse JSON, if any",
							"let json = {};",
							"try {",
							"  json = pm.response.json();",
							"} catch (e) {",
							"  json = {};",
							"}",
							"",
							"// Validate Status Code (200 OK)",
							"pm.test(\"Status code is 200\", function () {",
							"  pm.response.to.have.status(200);",
							"});",
							"",
							"// Only proceed if status is 200",
							"if (pm.response.code === 200) {",
							"  // Extract fields",
							"  const token = json.token;",
							"",
							"// Field validations",
							"pm.test(\"Response has expected token\", function () {",
							"  pm.expect(json).to.be.an(\"object\");",
							"  pm.expect(json, \"Missing token\").to.have.property(\"token\");",
							"  pm.expect(json.token, \"'token' should be a string\").to.be.a(\"string\");",
							"  pm.expect(String(json.token).trim().length, \"'token' should not be empty\").to.be.greaterThan(0);",
							"  pm.globals.set(\"team2_auth_token\", token);",
							"});",
							"",
							"pm.test(\"Response has expected type\", function () {",
							"  pm.expect(json).to.be.an(\"object\");",
							"  pm.expect(json, \"Missing type\").to.have.property(\"type\");",
							"  pm.expect(json.type, \"'type' should be a string\").to.be.a(\"string\");",
							"});",
							"",
							"pm.test(\"Response has expected userId\", function () {",
							"  pm.expect(json).to.be.an(\"object\");",
							"  pm.expect(json, \"Missing userId\").to.have.property(\"userId\");",
							"  pm.expect(json.userId, \"'userId' should be a string\").to.be.a(\"string\");",
							"});",
							"",
							"pm.test(\"Response has expected email\", function () {",
							"  pm.expect(json).to.be.an(\"object\");",
							"  pm.expect(json, \"Missing email\").to.have.property(\"email\");",
							"  pm.expect(json.email, \"'email' should be a string\").to.be.a(\"string\");",
							"});",
							"",
							"pm.test(\"Response has expected roles (array of non-empty strings)\", function () {",
							"  pm.expect(json).to.be.an(\"object\");",
							"  pm.expect(json, \"Missing roles\").to.have.property(\"roles\");",
							"  pm.expect(json.roles, \"'roles' should be an array\").to.be.an(\"array\");",
							"  pm.expect(json.roles.length, \"'roles' should not be empty\").to.be.greaterThan(0);",
							"  json.roles.forEach((r, i) => {",
							"    pm.expect(r, `roles[${i}] should be a string`).to.be.a(\"string\");",
							"    pm.expect(r.trim().length, `roles[${i}] should not be empty`).to.be.greaterThan(0);",
							"  });",
							"});",
							"",
							"pm.test(\"passwordExpired is a boolean\", function () {",
							"  pm.expect(json).to.be.an(\"object\");",
							"  pm.expect(json, \"Missing passwordExpired\").to.have.property(\"passwordExpired\");",
							"  pm.expect(json.passwordExpired, \"'passwordExpired' should be a boolean\").to.be.a(\"boolean\");",
							"});",
							"",
							"pm.test('status is either \"Active\" or \"Inactive\"', function () {",
							"  pm.expect(json).to.be.an(\"object\");",
							"  pm.expect(json, \"Missing status\").to.have.property(\"status\");",
							"  pm.expect(json.status, \"status should be a string\").to.be.a(\"string\");",
							"  const allowed = [\"Active\", \"Inactive\"];",
							"  pm.expect(allowed, `status must be one of ${allowed.join(\", \" )}`).to.include(json.status);",
							"});",
							"}",
							"if (pm.response.code === 400) {",
							"    //Extract message from response body",
							"    let errorMsg = json.message;",
							"",
							"    pm.test(\"Bad Request, Invalid details : \" + errorMsg, function () {",
							"        pm.response.to.have.status(400);",
							"    });",
							"}"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
