			"item": [
				{
					"name": "400CSV User Sign In",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "cedde928-261b-41b5-9445-6ddec37b44d6",
								"exec": [
									"\r",
									"let resBody = {};\r",
									"try {\r",
									"    resBody = pm.response.json();\r",
									"} catch (e) {\r",
									"    // Response is not JSON or empty\r",
									"    resBody = {};\r",
									"}\r",
									"// Extract token from response\r",
									"let token = resBody.token;\r",
									"\r",
									"pm.test(\"Content-Type header is present\", function () {\r",
									"    pm.response.to.have.header(\"Content-Type\");\r",
									"});\r",
									"if (pm.response.code === 400) {\r",
									"    //Extract message from response body\r",
									"    let errorMsg = resBody.message;\r",
									"\r",
									"    pm.test(\"Bad Request, Invalid details : \" + errorMsg, function () {\r",
									"        pm.response.to.have.status(400);\r",
									"    });\r",
									"}\r",
									"\r",
									"    if (pm.response.code === 415) {\r",
									"    let errorMsg = resBody.message || \"No message provided\";\r",
									"\r",
									"    pm.test(\"UNSUPPORTED_MEDIA_TYPE : \" + errorMsg, function () {\r",
									"        pm.response.to.have.status(415);\r",
									"    });\r",
									"}\r",
									"\r",
									"// Only proceed if status is 200\r",
									"if (pm.response.code === 200) {\r",
									"  // Extract fields\r",
									"  const token = resBody.token;\r",
									"  const tokenType = resBody.type; // e.g., \"Bearer\"\r",
									"\r",
									"pm.test(\"Existing User logged in successfully\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"  // Basic token presence check\r",
									"  pm.test(\"Token is present in response\", function () {\r",
									"    pm.expect(token, \"Response did not contain 'token' field\").to.exist;\r",
									"  });\r",
									"\r",
									"  // Save token and type to collectionVariables\r",
									"  if (token) {\r",
									"    pm.collectionVariables.set(\"auth_token\", token);\r",
									"    if (tokenType) pm.collectionVariables.set(\"auth_token_type\", tokenType);\r",
									"    pm.test(\"Token captured and saved\", function () {\r",
									"      pm.expect(pm.collectionVariables.get(\"auth_token\")).to.equal(token);\r",
									"    });\r",
									"    console.log(\"Captured token:\", tokenType ? `${tokenType} ${token}` : token);\r",
									"  }\r",
									"}"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						},
