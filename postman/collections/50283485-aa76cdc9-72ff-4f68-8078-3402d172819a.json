		{
			"name": "Cerate Skill Using Admin User",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "0eae1d0b-5362-47fb-9a81-0f8581057822",
						"exec": [
							"// Function to generate random string of given length\r",
							"function generateRandomString(length) {\r",
							"    let chars = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';\r",
							"    let result = '';\r",
							"    for (let i = 0; i < length; i++) {\r",
							"        result += chars.charAt(Math.floor(Math.random() * chars.length));\r",
							"    }\r",
							"    return result;\r",
							"}\r",
							"\r",
							"// Generate random skillName\r",
							"let randomSkill = generateRandomString(8);\r",
							"pm.environment.set(\"randomSkill\", randomSkill);\r",
							"console.log(\"Generated SkillName:\", randomSkill);\r",
							"\r",
							"// Set timestamps (optional, if you want dynamic values)\r",
							"let now = new Date().toISOString();\r",
							"pm.environment.set(\"creationTime\", now);\r",
							"pm.environment.set(\"lastModTime\", now);\r",
							"console.log(\"Timestamps set:\", now);\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "c55f0f30-1a4d-4119-91e3-32896650dd2a",
						"exec": [
							"let responseJson = {};\r",
							"try {\r",
							"    responseJson = pm.response.json();\r",
							"} catch (e) {\r",
							"    console.error(\"Response is not valid JSON:\", e);\r",
							"}\r",
							"\r",
							"console.log(\"Full response JSON:\", responseJson);\r",
							"\r",
							"// Extract skillId\r",
							"let skillIdFromResponse = responseJson.skillId || null;\r",
							"if (skillIdFromResponse) {\r",
							"    pm.environment.set(\"Skillid\", skillIdFromResponse);\r",
							"    console.log(\"Skillid saved:\", skillIdFromResponse);\r",
							"}\r",
							"\r",
							"// Status code test\r",
							"pm.test(\"Status code is 201 or 400\", function () {\r",
							"    pm.expect([201, 400]).to.include(pm.response.code);\r",
							"});\r",
							"\r",
							"// Validate skillName if created\r",
							"if (pm.response.code === 201) {\r",
							"    let skillNameSent = pm.environment.get(\"randomSkill\");\r",
							"    pm.test(\"skillName in response matches sent skillName\", function () {\r",
							"        pm.expect(responseJson.skillName).to.eql(skillNameSent);\r",
							"    });\r",
							"}\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
