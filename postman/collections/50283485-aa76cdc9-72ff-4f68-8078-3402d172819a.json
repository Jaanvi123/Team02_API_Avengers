			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "0eae1d0b-5362-47fb-9a81-0f8581057822",
						"exec": [
							"// Function to generate random string of given length\r",
							"function generateRandomString(length) {\r",
							"    let chars = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';\r",
							"    let result = '';\r",
							"    for (let i = 0; i < length; i++) {\r",
							"        result += chars.charAt(Math.floor(Math.random() * chars.length));\r",
							"    }\r",
							"    return result;\r",
							"}\r",
							"\r",
							"// Generate a random SkillName\r",
							"let randomSkill = generateRandomString(8); // You can change 8 to any length\r",
							"\r",
							"// Set it as a Postman variable\r",
							"pm.variables.set(\"randomSkill\", randomSkill);\r",
							"console.log(\"Generated SkillName:\", randomSkill);"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "c55f0f30-1a4d-4119-91e3-32896650dd2a",
						"exec": [
							"let responseJson = {};\r",
							"try {\r",
							"    responseJson = pm.response.json();\r",
							"} catch (e) {\r",
							"    console.error(\"Response is not valid JSON:\", e);\r",
							"}\r",
							"\r",
							"console.log(\"Full response JSON:\", responseJson);\r",
							"\r",
							"// Extract skillName correctly (case-sensitive)\r",
							"let skillNameFromResponse = responseJson.skillName || null;\r",
							"console.log(\"SkillName from response:\", skillNameFromResponse);\r",
							"\r",
							"// Status code test\r",
							"pm.test(\"Status code is 201 or 400\", function () {\r",
							"    pm.expect([201, 400]).to.include(pm.response.code);\r",
							"});\r",
							"\r",
							"// Check if skillName matches the one sent\r",
							"if (pm.response.code === 201) {\r",
							"    let skillNameSent = pm.variables.get(\"randomSkill\");\r",
							"    pm.test(\"skillName in response matches sent skillName\", function () {\r",
							"        pm.expect(skillNameFromResponse).to.eql(skillNameSent);\r",
							"    });\r",
							"}\r",
							"\r",
							"// Log error for 400\r",
							"if (pm.response.code === 400 && responseJson.error) {\r",
							"    console.log(\"Error message from response:\", responseJson.error);\r",
							"}\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
