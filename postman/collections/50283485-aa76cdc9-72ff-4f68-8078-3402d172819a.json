							"    console.log(\"Saved skillMasterName:\", skillNamePicked);\r",
							"\r",
							"    pm.test(\"skillMasterName variable set from response\", function () {\r",
							"        pm.expect(skillNamePicked).to.not.be.undefined;\r",
							"    });\r",
							"}\r",
							"\r",
							"//  Handle 400 Bad Request\r",
							"if (pm.response.code === 400) {\r",
							"    pm.test(\"Bad Request - Invalid input parameters\", function () {\r",
							"        pm.expect(pm.response.code).to.eql(400);\r",
							"    });\r",
							"    if (responseJson.error) {\r",
							"        console.log(\"Error message from response:\", responseJson.error);\r",
							"    }\r",
							"}\r",
							"\r",
							"//  Handle 404 Not Found\r",
							"if (pm.response.code === 404) {\r",
							"    const responseBody = pm.response.text();\r",
							"    if (responseBody === \"Invalid endpoint\") {\r",
							"        pm.test(\"Invalid Endpoint Check\", function () {\r",
							"            pm.expect(pm.response.code).to.eql(404);\r",
							"        });\r",
							"    } else {\r",
							"        pm.test(\"Not Found - Check Input Parameters\", function () {\r",
							"            pm.expect(pm.response.code).to.eql(404);\r",
							"        });\r",
							"        console.log(\"404 response body:\", responseBody);\r",
							"    }\r",
							"}\r",
							"\r",
							"//  Optional: Validate response structure if 200 OK\r",
							"if (pm.response.code === 200 && Array.isArray(responseJson)) {\r",
							"    pm.test(\"Response contains skill objects\", function () {\r",
							"        pm.expect(responseJson.length).to.be.above(0);\r",
							"        pm.expect(responseJson[0]).to.have.property(\"skillId\");\r",
							"        pm.expect(responseJson[0]).to.have.property(\"skillName\");\r",
							"        pm.expect(responseJson[0]).to.have.property(\"creationTime\");\r",
							"        pm.expect(responseJson[0]).to.have.property(\"lastModTime\");\r",
