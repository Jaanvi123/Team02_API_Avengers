			"name": "addnewskill",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "aa635f13-9df9-4a69-acfa-e9f00118b36f",
						"exec": [
							"\r",
							"\r",
							"// Generate random alphabetic string\r",
							"function randomString(len) {\r",
							"    const chars = \"ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz\";\r",
							"    let result = \"\";\r",
							"    for (let i = 0; i < len; i++) {\r",
							"        result += chars.charAt(Math.floor(Math.random() * chars.length));\r",
							"    }\r",
							"    return result;\r",
							"}\r",
							"\r",
							"// Create unique skill name\r",
							"let skillName = \"Skill-\" + randomString(5);\r",
							"pm.environment.set(\"skillName\", skillName);"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "c87b083f-c804-4716-adc7-9e1a3d4efb9a",
						"exec": [
							"// If success (200 OK)\r",
							"if (pm.response.code === 200) {\r",
							"\r",
							"    pm.test(\"Response contains required fields\", function () {\r",
							"        pm.expect(res).to.have.property(\"skillId\");\r",
							"        pm.expect(res).to.have.property(\"skillName\");\r",
							"        pm.expect(res).to.have.property(\"creationTime\");\r",
							"        pm.expect(res).to.have.property(\"lastModTime\");\r",
							"    });\r",
							"     // Save skillId globally for chaining\r",
							"    pm.globals.set(\"skillId\", res.skillId);\r",
							"}\r",
							"// If Bad Request (400)\r",
							"if (pm.response.code === 400) {\r",
							"    pm.test(\"Check error message exists\", function () {\r",
							"        pm.expect(res).to.have.property(\"message\");\r",
							"    });\r",
							"\r",
							"    console.warn(\"Bad Request:\", res.message);\r",
							"}"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
