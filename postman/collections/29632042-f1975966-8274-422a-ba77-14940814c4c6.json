	"item": [
		{
			"name": "CreateUser",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "fd1a2aec-714d-4824-91b3-010e5836a37c",
						"exec": [
							"//Extract response from Json body of API response\r",
							"const response = pm.response.json();\r",
							"\r",
							"pm.test(\"User successfully created\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"    //Extract userId from user object in response\r",
							"    pm.globals.set(\"userID\", response.user.userId)\r",
							"});\r",
							"if (pm.response.code === 400) {\r",
							"    //Extract message from response body\r",
							"    let errorMsg = response.message;\r",
							"\r",
							"    pm.test(\"Bad Request, Invalid details : \" + errorMsg, function () {\r",
							"        pm.response.to.have.status(400);\r",
							"    });\r",
							"}\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"id": "0c137e93-f629-4515-b654-324eb28c662d",
						"exec": [
							"const rawPhoneNumber = pm.variables.replaceIn('{{$randomPhoneNumber}}');\r",
							"console.log(\"raw phone:\" + rawPhoneNumber);\r",
							"\r",
							"//Replace '-' from the random generated phone number.\r",
							"const cleanedPhoneNumber = +rawPhoneNumber.replace(/-/g, '');\r",
							"pm.collectionVariables.set(\"randomPhNumber\", \"+1 \" + cleanedPhoneNumber);\r",
							"console.log(pm.collectionVariables.get('randomPhNumber'));\r",
							"\r",
							"//Generate random email. API only accepts @gmail.com\r",
							"pm.globals.set('userEmail', pm.variables.replaceIn(\"{{$randomFirstName}}\") + \"@gmail.com\");\r",
							"console.log(pm.globals.get('userEmail'));\r",
							"console.log(pm.request.body.raw);\r",
							"\r",
							"const request = pm.request.body.toJSON();\r",
							"\r",
							"pm.test(\"Mandatory fields in Response body\", function () {\r",
							"    pm.expect(request).to.have.property(\"user.userFirstName\");\r",
							"    pm.expect(request).to.have.property(\"user.userLastName\");\r",
							"    pm.expect(request).to.have.property(\"user.userLocation\");\r",
							"    pm.expect(request).to.have.property(\"user.userTimeZone\");\r",
							"    pm.expect(request).to.have.property(\"user.userVisaStatus\");\r",
							"    pm.expect(request).to.have.property(\"user.roleId\");\r",
							"    pm.expect(request).to.have.property(\"user.userRoleStatus\");\r",
							"    pm.expect(request).to.have.property(\"user.loginStatus\");\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"id": "29632042-b636a9e8-1137-4374-baf0-9c661fed012a",
