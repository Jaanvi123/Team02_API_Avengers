				"body": {
					"mode": "raw",
					"raw": "{\r\n\"userComments\": \"string\",\r\n\"userEduPg\": \"string\",\r\n\"userEduUg\": \"string\",\r\n\"userFirstName\": \"YogiS\",\r\n\"userLastName\": \"Sh\",\r\n\"userLinkedinUrl\": \"www.linkedin.com/123\",\r\n\"userLocation\": \"USA\",\r\n\"userMiddleName\": \"S\",\r\n\"userPhoneNumber\": \"{{randomPhNumber}}\",\r\n\"userRoleMaps\": [\r\n{\r\n\"roleId\": \"R03\",\r\n\"userRoleStatus\": \"Active\"\r\n}\r\n],\r\n\"userTimeZone\": \"CST\",\r\n\"userVisaStatus\": \"H4\",\r\n\"userLogin\": {\r\n\"userLoginEmail\": \"{{userEmail}}\",\r\n\"loginStatus\" : \"Active\",\r\n\"status\": \"Active\"\r\n} \r\n}\r\n",
					"options": {
