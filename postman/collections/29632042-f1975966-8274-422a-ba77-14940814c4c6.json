		{
			"name": "CreateUser",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "fd1a2aec-714d-4824-91b3-010e5836a37c",
						"exec": [
							"const response = pm.response.json();\r",
							"\r",
							"pm.test(\"User successfully created\", function () { \r",
							"    pm.response.to.have.status(201);\r",
							"    pm.collectionVariables.set(\"userID\", response.user.userId)\r",
							"});\r",
							"if (pm.response.code === 400) {\r",
							"let errorMsg = response.message;\r",
							"\r",
							"pm.test(\"Bad Request, Invalid data : \"+errorMsg, function () { \r",
							"    pm.response.to.have.status(400);\r",
							"});\r",
							"\r",
							"}"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"id": "0c137e93-f629-4515-b654-324eb28c662d",
						"exec": [
							"const rawPhoneNumber = pm.variables.replaceIn('{{$randomPhoneNumber}}');\r",
							"console.log(\"raw phone:\"+rawPhoneNumber);\r",
							"\r",
							"//Replace '-' from the random generated phone number.\r",
							"const cleanedPhoneNumber = +rawPhoneNumber.replace(/-/g, ''); \r",
							"pm.collectionVariables.set(\"randomPhNumber\", \"+1 \"+cleanedPhoneNumber);\r",
							"console.log(pm.collectionVariables.get('randomPhNumber'));\r",
							"\r",
							"//Generate random email. API only accepts @gmail.com\r",
							"    pm.collectionVariables.set('userEmail', pm.variables.replaceIn(\"{{$randomFirstName}}\")+\"@gmail.com\");\r",
							"    console.log(pm.collectionVariables.get('userEmail'));\r",
							"    console.log(pm.request.body.raw);"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
